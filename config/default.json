{
    "jwt": {
        "options": {
            "expiresIn": 365000
        }
    },
    "validation": {
        "options": {
            "abortEarly": true,
            "allowUnknown": true,
            "convert": true
        }
    },
    "image": {
        "validMimes": [
            "image/jpeg",
            "image/png",
            "image/svg+xml",
            "image/gif",
            "image/webp"
        ]
    },
    "elasticSearch": {
        "productsIndex": "products"
    }
}