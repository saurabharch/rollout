{
    "name": "rollout",
    "version": "1.0.01",
    "description": "Rollout is a Push Notification Engine,  Application is Written in Node.js and have Restful API features.",
    "main": "src/index.js",
    "scripts": {
        "copy": "copyfiles -u 1 src/**/**/* src/**/* src/* build",
        "//pretest": "mongodb-runner start",
        "watchenv": "nodemon --watch .env --watch ./confenv/.env ./bin/www",
        "build:dev": "npm run copy && workbox injectManifest src/workbox-config.js",
        "build": "babel src --out-dir build",
        "//testdata": "node src/lib/testdata.js",
        "lint": "node ./node_modules/eslint/bin/eslint.js ./src --fix",
        "precommit": "node ./node_modules/eslint/bin/eslint.js ./src",
        "//test": "jest",
        "demo:test": "ava",
        "start": "node build/index.js",
        "restart": "pm2 startOrRestart ecosystem.config.js",
        "pro:deploy": "pm2 deploy ecosystem.config.js",
        "dev": "npm-run-all -p dev:*",
        "dev:tailwind": "postcss src/public/css/tailwind.css -o src/public/css/style1.css",
        "dev:server": "nodemon src/index.js --exec babel-node",
        "dev:worker": "nodemon src/worker/queue.js --exec babel-node",
        "//dev:build": "gulp watch",
        "//dev:test": "ava",
        "//dev:deploy": "pm2 deploy ecosystem.config.js development",
        "format": "prettier --write './src/**/*.js'",
        "//lint": "eslint -c ./eslintrc.json './src/**/*.js'",
        "index-products": "node src/scripts/indexing.js",
        "update:packages": "npm clean-install && npm install && npm update --save/--save-dev -f",
        "update:geodate": "npm run-script updatedb-force license_key=sClIi7oTlPcCEcn8",
        "update:geodb": "node ./node_modules/geoip-lite/scripts/updatedb.js license_key=sClIi7oTlPcCEcn8",
        "posttest": "mongodb-runner stop",
        "preinstall": "if [ $ENABLE_ENTERPRISE_MODULE ]; then npm run enable-ent; fi",
        "//heroku-prebuild": "if [ $ENABLE_ENTERPRISE_MODULE ]; then npm run enable-ent; fi",
        "//enable-ent": "echo //registry.npmjs.org/:_authToken=${NPM_TOKEN} > .npmrc"
    },
    "redisMemoryServer": {
        "downloadDir": "/path/to/redis/binaries",
        "version": "6.0.10",
        "debug": "1",
        "downloadMirror": "url",
        "disablePostinstall": "1",
        "systemBinary": "/usr/local/bin/redis-server"
    },
    "ava": {
        "serial": true,
        "files": [
            "./src/test/specs/*.js"
        ],
        "timeout": "10s",
        "verbose": true,
        "environmentVariables": {
            "NODE_ENV": "test"
        }
    },
